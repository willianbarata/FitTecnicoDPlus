<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="FitTecnicoDPlus.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:paginaedicao="clr-namespace:FitTecnicoDPlus.MVVM.View"
    xmlns:paginainicial="clr-namespace:FitTecnicoDPlus.View"
    Shell.FlyoutBehavior="Disabled">
    <ShellContent
        Title="Lista de Clientes"
        ContentTemplate="{DataTemplate paginainicial:ListarCliente}"
        Route="ListarCliente" />
    <ShellContent
        Title="CadastrarCliente"
        ContentTemplate="{DataTemplate paginainicial:CadastrarCliente}"
        Route="CadastrarCliente" />
    <ShellContent
        Title="EditarCliente"
        ContentTemplate="{DataTemplate paginaedicao:EditarCliente}"
        Route="EditarCliente" />
</Shell>